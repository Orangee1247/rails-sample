<p id="notice"><%= notice %></p>

<h1>Listing Undos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @undos.each do |undo| %>
      <tr>
        <td><%= link_to 'Show', undo %></td>
        <td><%= link_to 'Edit', edit_undo_path(undo) %></td>
        <td><%= link_to 'Destroy', undo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Undo', new_undo_path %>
